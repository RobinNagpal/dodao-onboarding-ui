{
  "name": "snapshot",
  "version": "0.1.4",
  "scripts": {
    "preinstall": "yarn run init-submodules",
    "dev": "yarn init-submodules && mkdir -p src/graphql/generated && yarn build:graphql && vite",
    "build": "mkdir -p src/graphql/generated && yarn build:graphql && vite build",
    "lint": "eslint \"./src/*/**/*.{ts,vue,json}\" --fix",
    "postinstall": "patch-package && husky install",
    "init-submodules": "git submodule update --init",
    "build:graphql": "apollo codegen:generate src/graphql/generated/graphqlDocs.ts  --localSchemaFile=schema.graphql --includes='src/**/*.graphql' --target typescript --outputFlat"
  },
  "dependencies": {
    "@apollo/client": "^3.5.7",
    "@ensdomains/eth-ens-namehash": "^2.0.15",
    "@ethersproject/abi": "^5.4.0",
